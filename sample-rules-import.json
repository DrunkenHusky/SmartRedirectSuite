[
  {
    "matcher": "/old-page",
    "targetUrl": "https://example.com/new-page",
    "redirectType": "wildcard",
    "infoText": "Exact Page Redirect (Wildcard)",
    "autoRedirect": false,
    "discardQueryParams": false,
    "forwardQueryParams": true
  },
  {
    "matcher": "/legacy-section",
    "targetUrl": "/modern-section",
    "redirectType": "partial",
    "infoText": "Section Redirect: Keeps 'id' parameter and adds static 'source=migration'",
    "autoRedirect": true,
    "discardQueryParams": true,
    "forwardQueryParams": false,
    "keptQueryParams": [
      { "keyPattern": "id" }
    ],
    "staticQueryParams": [
      { "key": "source", "value": "migration" }
    ]
  },
  {
    "matcher": "/products",
    "targetUrl": "/shop/products",
    "redirectType": "partial",
    "infoText": "Product Redirect: Replaces 'old-cat' with 'new-cat' in the final URL",
    "autoRedirect": false,
    "discardQueryParams": false,
    "forwardQueryParams": false,
    "searchAndReplace": [
      { "search": "old-cat", "replace": "new-cat", "caseSensitive": false }
    ]
  }
]